public class SpringTest {

	public static void main(String[] args) {
		String path = "part01_basic/di.xml";
		ApplicationContext context = new ClassPathXmlApplicationContext(path);
		Service svc = (Service) context.getBean("svc");
		svc.prn();

	} // end main()

} // end class
